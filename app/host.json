{
    "version": "2.0",
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "enableLiveMetricsFilters": true
      }
    },
    "extensions": {
      "openai": {
        "searchProvider": {
          "type": "azureAiSearch",
          "isSemanticSearchEnabled": true,
          "useSemanticCaptions": false,
          "vectorSearchDimensions": 1536
        }
      }
    }
  }